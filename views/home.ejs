<%- include('partials/header') -%>
<h1>Home</h1>

<p><%=homeStartingContent %></p>

<% posts.forEach(elem => { %>
<div>
  <h1><%= elem.title %></h1>
  <p>
    <%= elem.content.substring(1, 100) + "..." %>
    <a href="/posts/<%=elem.title %>">Read more</a>
  </p>
</div>
<%}) %> <%- include('partials/footer') -%>
